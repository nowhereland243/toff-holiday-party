{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/v7_cosmos/index_v3.html"
    },
    {
      "source": "/(.*)",
      "destination": "/v7_cosmos/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
