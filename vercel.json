{
  "name": "toff-holiday",
  "version": 2,
  "rewrites": [
    {
      "source": "/intro",
      "destination": "/v7/index_v3.html"
    },
    {
      "source": "/toff2025-holiday-donation",
      "destination": "/v7/index_v3.html"
    },
    {
      "source": "/assets/:path*",
      "destination": "/v7/assets/:path*"
    },
    {
      "source": "/audio/:path*",
      "destination": "/v7/audio/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "/v7/images/:path*"
    },
    {
      "source": "/fonts/:path*",
      "destination": "/v7/fonts/:path*"
    },
    {
      "source": "/scripts/:path*",
      "destination": "/v7/scripts/:path*"
    },
    {
      "source": "/styles/:path*",
      "destination": "/v7/styles/:path*"
    },
    {
      "source": "/manifest.json",
      "destination": "/v7/manifest.json"
    },
    {
      "source": "/(vortex.*)",
      "destination": "/v7/$1"
    },
    {
      "source": "/background-noise.png",
      "destination": "/v7/background-noise.png"
    },
    {
      "source": "/Auto-Captions-V6.mp4",
      "destination": "/v7/Auto-Captions-V6.mp4"
    },
    {
      "source": "/(Favorit-.*)",
      "destination": "/v7/$1"
    },
    {
      "source": "/",
      "destination": "/v7/index_v3.html"
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
