{
  "name": "toff-holiday",
  "version": 2,
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/donatenow",
      "destination": "/donation_now/dist/index.html"
    },
    {
      "source": "/donatenow/(.*)",
      "destination": "/donation_now/dist/$1"
    },
    {
      "source": "/donation",
      "destination": "/donation/dist/main.html"
    },
    {
      "source": "/donation/(.*)",
      "destination": "/donation/dist/$1"
    },
    {
      "source": "/v1",
      "destination": "/v1/index.html"
    },
    {
      "source": "/v1/:path*",
      "destination": "/v1/:path*"
    },
    {
      "source": "/intro",
      "destination": "/v7/index_v3.html"
    },
    {
      "source": "/favicon.ico",
      "destination": "/v7/favicon.ico"
    },
    {
      "source": "/toff2025-holiday-donation",
      "destination": "/v7/index_v3.html"
    },
    {
      "source": "/assets/:path*",
      "destination": "/v7/assets/:path*"
    },
    {
      "source": "/audio/:path*",
      "destination": "/v7/audio/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "/v7/images/:path*"
    },
    {
      "source": "/fonts/:path*",
      "destination": "/v7/fonts/:path*"
    },
    {
      "source": "/scripts/:path*",
      "destination": "/v7/scripts/:path*"
    },
    {
      "source": "/styles/:path*",
      "destination": "/v7/styles/:path*"
    },
    {
      "source": "/manifest.json",
      "destination": "/v7/manifest.json"
    },
    {
      "source": "/GTSuperDisplay-Light.woff2",
      "destination": "/v7/fonts/gt-super/GTSuperDisplay-Light.woff2"
    },
    {
      "source": "/(vortex.*)",
      "destination": "/v7/$1"
    },
    {
      "source": "/background-noise.png",
      "destination": "/v7/background-noise.png"
    },
    {
      "source": "/Auto-Captions-V6.mp4",
      "destination": "/v7/Auto-Captions-V6.mp4"
    },
    {
      "source": "/(Favorit-.*)",
      "destination": "/v7/$1"
    },
    {
      "source": "/Asset_12222.svg",
      "destination": "/v7/Asset_12222.svg"
    },
    {
      "source": "/",
      "destination": "/v7/index_v3.html"
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
